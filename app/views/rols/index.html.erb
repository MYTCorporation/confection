<h1>Listing rols</h1>

<table>
  <thead>
    <tr>
      <th>Administrador</th>
      <th>Instructor</th>
      <th>Aprendiz</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rols.each do |rol| %>
      <tr>
        <td><%= rol.Administrador %></td>
        <td><%= rol.Instructor %></td>
        <td><%= rol.Aprendiz %></td>
        <td><%= link_to 'Show', rol %></td>
        <td><%= link_to 'Edit', edit_rol_path(rol) %></td>
        <td><%= link_to 'Destroy', rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol', new_rol_path %>
